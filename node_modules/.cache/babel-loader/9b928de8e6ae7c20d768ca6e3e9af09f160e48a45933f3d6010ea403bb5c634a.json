{"ast":null,"code":"import { getData } from './api_layer';\nimport { mapState, mapMutations } from 'vuex';\nexport default {\n  name: 'App',\n  data() {\n    return {\n      searchQuery: '',\n      searchResults: null,\n      mediaTypeSwitch: false\n    };\n  },\n  computed: {\n    ...mapState({\n      mediaType: state => state.mediaType\n    })\n  },\n  watch: {\n    async searchQuery(query) {\n      if (query) {\n        const {\n          results\n        } = await getData(`https://api.themoviedb.org/3/search/movie?query=${query}`);\n        this.searchResults = results;\n      } else {\n        this.searchResults = null;\n      }\n    },\n    mediaTypeSwitch(type) {\n      this.SetMediaType(type ? 'series' : 'movies');\n    }\n  },\n  methods: {\n    ...mapMutations(['SetMediaType']),\n    clickedOutside() {\n      console.log('Hello');\n      this.searchQuery = '';\n    }\n  }\n};","map":{"version":3,"names":["getData","mapState","mapMutations","name","data","searchQuery","searchResults","mediaTypeSwitch","computed","mediaType","state","watch","query","results","type","SetMediaType","methods","clickedOutside","console","log"],"sources":["/Users/cinnamonarts/Documents/jayflix/src/App.vue"],"sourcesContent":["<style lang=\"scss\">\n @import './assets/styles/app.scss';\n @import './assets/styles/style_guide.scss';\n</style>\n\n<template>\n  <nav>\n    <ul>\n      <li><a class=\"active\" href=\"/\"><i class=\"fa fa-home\" /></a></li>\n    </ul>\n    <div class=\"media-switch\">\n      <h3>MOVIES</h3>\n      <label class=\"switch\">\n        <input type=\"checkbox\" v-model=\"mediaTypeSwitch\">\n        <span class=\"slider\">\n            <span class=\"circle\"><i class=\"fa\" :class=\"mediaTypeSwitch ? 'fa-tv' : 'fa-film'\" /></span>\n        </span>\n      </label>\n      <h3>SERIES</h3>\n    </div>\n    <div class=\"search\" :click-outisde=\"clickedOutside\">\n        <div class=\"search-bar\">\n          <input\n            type=\"text\" \n            placeholder=\"Search ...\"\n            v-model=\"searchQuery\">\n          <i class=\"fa fa-search\"></i>\n        </div>\n        <div v-if=\"searchResults\" class=\"list-group\">\n          <li v-for=\"(result, index) in searchResults\" :key=\"index\">\n            {{ result.title }}\n          </li>\n        </div>\n      </div>\n  </nav>\n  <div id=\"container\">\n    <router-view/>\n  </div>\n</template>\n\n<script>\nimport { getData } from './api_layer';\nimport { mapState, mapMutations } from 'vuex';\n\nexport default {\n  name: 'App',\n  data () {\n    return {\n      searchQuery: '',\n      searchResults: null,\n      mediaTypeSwitch: false\n    }\n  },\n  computed: {\n    ...mapState({\n      mediaType: state => state.mediaType\n    })\n  },\n  watch: {\n    async searchQuery (query) {\n      if (query) {\n        const {results} = await getData(`https://api.themoviedb.org/3/search/movie?query=${query}`);\n        this.searchResults = results;\n      } else {\n        this.searchResults = null;\n      }\n    },\n    mediaTypeSwitch (type) {\n      this.SetMediaType(type ? 'series' : 'movies');\n    }\n  },\n  methods: {\n    ...mapMutations(['SetMediaType']),\n    clickedOutside () {\n      console.log('Hello');\n      this.searchQuery = '';\n    }\n  }\n}\n</script>\n"],"mappings":"AAyCA,SAASA,OAAM,QAAS,aAAa;AACrC,SAASC,QAAQ,EAAEC,YAAW,QAAS,MAAM;AAE7C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,aAAa,EAAE,IAAI;MACnBC,eAAe,EAAE;IACnB;EACF,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGP,QAAQ,CAAC;MACVQ,SAAS,EAAEC,KAAI,IAAKA,KAAK,CAACD;IAC5B,CAAC;EACH,CAAC;EACDE,KAAK,EAAE;IACL,MAAMN,WAAUA,CAAGO,KAAK,EAAE;MACxB,IAAIA,KAAK,EAAE;QACT,MAAM;UAACC;QAAO,IAAI,MAAMb,OAAO,CAAE,mDAAkDY,KAAM,EAAC,CAAC;QAC3F,IAAI,CAACN,aAAY,GAAIO,OAAO;MAC9B,OAAO;QACL,IAAI,CAACP,aAAY,GAAI,IAAI;MAC3B;IACF,CAAC;IACDC,eAAcA,CAAGO,IAAI,EAAE;MACrB,IAAI,CAACC,YAAY,CAACD,IAAG,GAAI,QAAO,GAAI,QAAQ,CAAC;IAC/C;EACF,CAAC;EACDE,OAAO,EAAE;IACP,GAAGd,YAAY,CAAC,CAAC,cAAc,CAAC,CAAC;IACjCe,