{"ast":null,"code":"// @ is an alias to /src\nimport ModalComponent from '@/components/ModalComponent.vue';\nimport { mapActions } from 'vuex';\nexport default {\n  name: 'HomeView',\n  components: {\n    ModalComponent\n  },\n  data() {\n    return {\n      posts: null,\n      videoPlayer: {\n        show: false,\n        videoId: null\n      }\n    };\n  },\n  async mounted() {\n    this.posts = await this.getLatestMovies();\n  },\n  methods: {\n    ...mapActions(['getLatestMovies']),\n    watchMovie(id) {\n      this.videoPlayer.show = true;\n      this.videoPlayer.videoId = id;\n    }\n  }\n};","map":{"version":3,"names":["ModalComponent","mapActions","name","components","data","posts","videoPlayer","show","videoId","mounted","getLatestMovies","methods","watchMovie","id"],"sources":["/Users/cinnamonarts/Documents/jayflix/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div>\n    <template v-if=\"posts\">\n      <div \n        v-for=\"(movie, i) in posts.results\" \n        :key=\"i\"\n        class=\"image_backdrop_container\"\n        :style=\"`background: url(https://image.tmdb.org/t/p/original${movie.backdrop_path})`\">\n        <div class=\"popular-movie-slider\">\n      \n          <img :src=\"`https://image.tmdb.org/t/p/original${movie.poster_path}`\" class=\"poster\">\n          \n          <div class=\"popular-movie-slider-content\">\n            <p class=\"release\">{{movie.release_date}}</p>\n            <h2 class=\"movie-name\">{{movie.original_title}}</h2>\n            <p class=\"desc\">{{movie.overview}}</p>\n            <div class=\"movie-btns\">\n              <button @click=\"watchMovie(movie.id)\"><span class=\"fa fa-play\" aria-hidden=\"true\"></span>&nbsp;Watch</button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </template>\n    <ModalComponent v-if=\"videoPlayer.show\" @close=\"videoPlayer.show = false\">\n      <template v-slot:body>\n        <iframe \n          style=\"width:100%; height: 500px;\"\n          :src=\"`https://vidsrc.me/embed/movie?tmdb=${videoPlayer.videoId}`\"\n          frameborder=\"0\"\n          allowfullscreen />\n      </template>\n    </ModalComponent>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport ModalComponent from '@/components/ModalComponent.vue';\nimport { mapActions } from 'vuex';\n\nexport default {\n  name: 'HomeView',\n  components: {\n    ModalComponent\n  },\n  data () {\n    return {\n      posts: null,\n      videoPlayer: {\n        show: false,\n        videoId: null\n      }\n    }\n  },\n  async mounted () {\n    this.posts = await this.getLatestMovies();\n  },\n  methods: {\n    ...mapActions(['getLatestMovies']),\n    watchMovie (id) {\n      this.videoPlayer.show = true;\n      this.videoPlayer.videoId = id;\n    }\n  }\n}\n</script>\n"],"mappings":"AAoCA;AACA,OAAOA,cAAa,MAAO,iCAAiC;AAC5D,SAASC,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,KAAK,EAAE,IAAI;MACXC,WAAW,EAAE;QACXC,IAAI,EAAE,KAAK;QACXC,OAAO,EAAE;MACX;IACF;EACF,CAAC;EACD,MAAMC,OAAMA,CAAA,EAAK;IACf,IAAI,CAACJ,KAAI,GAAI,MAAM,IAAI,CAACK,eAAe,CAAC,CAAC;EAC3C,CAAC;EACDC,OAAO,EAAE;IACP,GAAGV,UAAU,CAAC,CAAC,iBAAiB,CAAC,CAAC;IAClCW,UAASA,CAAGC,EAAE,EAAE;MACd,IAAI,CAACP,WAAW,CAACC,IAAG,GAAI,IAAI;MAC5B,IAAI,CAACD,WAAW,CAACE,OAAM,GAAIK,EAAE;IAC/B;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}